{
    "json.schemas": [
        {
            "url": "https://cdn.jsdelivr.net/npm/tsup/schema.json",
            "fileMatch": ["package.json", "tsup.config.json"]
        }
    ],
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "packages/core/docs": true
    },
    "typescript.tsdk": "node_modules/typescript/lib"
}
